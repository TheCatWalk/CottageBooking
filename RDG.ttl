@prefix cot: <http://users.jyu.fi/~kumapmxw/cottage-ontology.owl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix sswap: <http://sswapmeet.sswap.info/sswap/> .
@prefix resource: <http://127.0.0.1:8000/> .

resource:CottageBooking
    rdf:type sswap:Resource, cot:CottageBookingService ;
    sswap:providedBy resource:resourceProvider ;
    sswap:name "Aurora Cottage Booking Service" ;
    sswap:oneLineDescription "A service in which you can search and book cottages" ;

    sswap:operatesOn [
        rdf:type sswap:Graph ;

        sswap:hasMapping [
            rdf:type sswap:Subject, cot:BookingRequest;
            cot:booker_name "" ;
            cot:numberOfPlaces ""^^xsd:int ;
            cot:numberOfBedrooms ""^^xsd:int ;
            cot:cityName "" ;
            cot:distanceFromCity ""^^xsd:int ;
            cot:distanceFromLake ""^^xsd:int ;
            cot:startDate ""^^xsd:dateTime ;
            cot:duration ""^^xsd:int ;
            cot:maxShiftDays ""^^xsd:int ;

            sswap:mapsTo [
                rdf:type sswap:Object, cot:BookingCottage ;
                cot:booker_name "" ;
                cot:hasBookingID "" ;
                cot:hasImageURL "" ;
                cot:hasAddress "" ;
                cot:numberOfPlaces ""^^xsd:int ;
                cot:numberOfBedrooms ""^^xsd:int ;
                cot:cityName "" ;
                cot:distanceFromCity ""^^xsd:int ;
                cot:distanceFromLake ""^^xsd:int ;
                cot:startDate ""^^xsd:dateTime ;
                cot:endDate ""^^xsd:dateTime
            ]
        ]
    ] .
